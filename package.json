{
  "name": "pbff",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "build": "NODE_ENV=production gulp build",
    "start": "NODE_ENV=development gulp start",
    "test": "NODE_ENV=test jest --coverage"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write",
      "eslint --fix",
      "git add"
    ],
    "*.{css,pcss}": [
      "stylelint --fix",
      "git add"
    ],
    "*.{png,jpg,jpeg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "testURL": "https://sketch.pixiv.net/",
    "rootDir": "./src",
    "bail": false,
    "verbose": false,
    "timers": "fake",
    "cacheDirectory": "<rootDir>/../tmp/jest",
    "coverageDirectory": "<rootDir>/../tmp/coverage",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/../lib/setupTestFramework.js",
    "moduleNameMapper": {},
    "collectCoverageFrom": [
      "<rootDir>/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "axios": "0.18.0",
    "babel-polyfill": "6.26.0",
    "body-parser": "1.18.2",
    "classnames": "2.2.5",
    "clean-css": "4.1.11",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "domready": "1.0.8",
    "express": "4.16.2",
    "fbjs": "0.8.16",
    "fluxible": "1.4.0",
    "fluxible-addons-react": "0.2.14",
    "fluxible-router": "1.5.1",
    "helmet": "3.12.0",
    "hoist-non-react-statics": "2.5.0",
    "immutable": "3.8.2",
    "jsonwebtoken": "8.2.0",
    "lodash": "4.17.21",
    "minireset.css": "0.0.3",
    "morgan": "1.9.0",
    "pretty-error": "2.1.1",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-immutable-proptypes": "2.1.0",
    "react-immutable-pure-component": "1.1.1",
    "response-time": "2.3.2",
    "serialize-javascript": "1.4.0",
    "serve-favicon": "2.4.5",
    "source-map-support": "0.5.3",
    "ua-parser-js": "0.7.17"
  },
  "devDependencies": {
    "ansi-colors": "1.1.0",
    "assets-webpack-plugin": "3.5.1",
    "autoprefixer": "8.1.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "browser-sync": "2.23.6",
    "cheerio": "1.0.0-rc.2",
    "connect-query": "1.0.0",
    "css-loader": "0.28.10",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-private-props": "0.3.0",
    "eslint-plugin-react": "7.7.0",
    "fancy-log": "1.3.2",
    "file-loader": "1.1.11",
    "fs-extra": "5.0.0",
    "gulp": "3.9.1",
    "husky": "0.14.3",
    "imagemin-lint-staged": "0.3.0",
    "isomorphic-style-loader": "4.0.0",
    "jest-cli": "22.4.2",
    "lint-staged": "7.0.0",
    "multimeter": "0.1.1",
    "null-loader": "0.1.1",
    "postcss-calc": "6.0.1",
    "postcss-color-function": "4.0.1",
    "postcss-custom-media": "6.0.0",
    "postcss-custom-properties": "7.0.0",
    "postcss-custom-selectors": "4.0.1",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.1",
    "postcss-media-minmax": "3.0.0",
    "postcss-nested": "3.0.0",
    "postcss-nesting": "4.2.1",
    "postcss-selector-not": "3.0.1",
    "prettier": "1.11.1",
    "prettier-eslint": "8.8.1",
    "prettier-eslint-cli": "4.7.1",
    "prettier-stylelint": "0.4.2",
    "react-deep-force-update": "2.1.1",
    "react-dev-utils": "5.0.0",
    "regenerator-runtime": "0.11.1",
    "run-sequence": "2.2.1",
    "stylelint": "9.1.1",
    "stylelint-config-standard": "18.2.0",
    "svg-inline-loader": "0.8.0",
    "svgo": "1.0.5",
    "url-loader": "1.0.1",
    "webpack": "4.1.1",
    "webpack-dev-middleware": "3.0.1",
    "webpack-hot-middleware": "2.21.2",
    "webpack-node-externals": "1.6.0",
    "workbox-webpack-plugin": "2.1.3"
  }
}
